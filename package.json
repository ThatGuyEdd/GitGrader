{
  "name": "GitGrader",
  "version": "1.0.0",
  "main": "dist/electron.js",
  "description": "An Electron app to help combine assigner and grader together to make a friendly ui for teachers/graders to use.",
  "repository": "git@github.com:Uhuh/GitGrader.git",
  "author": "Dylan Warren <duwtgb@mst.edu>",
  "license": "MIT",
  "scripts": {
    "package": "yarn build && electron-builder ./dist/",
    "build": "webpack --config ./webpack.config.js",
    "start": "yarn build && electron dist/electron.js"
  },
  "build": {
    "appId": "com.gitgrader.app",
    "productName": "GitGrader",
    "directories": {
      "output": "build",
      "buildResources": "dist"
    },
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    }
  },
  "dependencies": {
    "@material-ui/core": "^4.9.5",
    "@material-ui/icons": "^4.9.1",
    "@types/electron": "^1.6.10",
    "@types/node": "^13.7.0",
    "@types/react": "^16.9.19",
    "@types/react-bootstrap": "^0.32.20",
    "@types/react-dom": "^16.9.5",
    "@types/react-router-dom": "^5.1.3",
    "@types/styled-components": "^5.0.1",
    "axios": "^0.19.2",
    "components": "^0.1.0",
    "electron-is-dev": "^1.2.0",
    "html-webpack-plugin": "^3.2.0",
    "node": "^13.7.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "styled": "^1.0.0",
    "styled-components": "^5.0.1",
    "ts-loader": "^6.2.1",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "devDependencies": {
    "asar": "^3.0.3",
    "electron": "^8.0.0",
    "electron-builder": "^22.5.1",
    "electron-packager": "^14.2.1"
  }
}
